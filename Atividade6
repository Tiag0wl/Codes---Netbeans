package com.mycompany.algoritimosatvividades;

import java.util.Scanner;
public class Atividade6 {

    public static void main(String[] args) {
    Scanner leitor = new Scanner (System.in);
    
    System.out.println("Digite a sua velocidade media km/h");
           double velocidade;
           velocidade = leitor.nextDouble();
           
    System.out.println("Digite por quantas horas voce andou nessa velocidade");
           double tempo;
           tempo = leitor.nextDouble();
           
           double distancia;
           distancia = ( velocidade * tempo );
           
           System.out.print("Voce percorreu ");      
           System.out.print( distancia );
           System.out.print("km."); 
    }
    
}
